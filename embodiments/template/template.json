        "camera": {
            "label": "Vision",
            "description": "",
            "parameters": [
                {
                    "label": "custom_name",
                    "description": "User defined name for the device.",
                    "type": "string"
                },
                {
                    "label": "disabled",
                    "description": "Disables the device when true.",
                    "type": "boolean",
                    "default": false
                },
                {
                    "label": "index",
                    "description": "An index to create cortical vision name such as iv00_C, iv01_C, etc.",
                    "type": "string"
                },
                {
                    "label": "mirror",
                    "description": "horizontal flip",
                    "type": "boolean",
                    "default": false
                },
                {
                    "label": "eccentricity_control",
                    "description": "Controls the eccentricity amount of the signal",
                    "type": "object",
                    "keys_type": "string",
                    "properties": {
                        "x_offset": {
                            "label": "X offset percentage",
                            "type": "percentage",
                            "default": 1,
                            "min": 1,
                            "max": 99
                        },
                        "y_offset": {
                            "label": "Y offset percentage",
                            "type": "percentage",
                            "default": 1,
                            "min": 1,
                            "max": 99
                        },
                        "require": false
                    }
                },
                {
                    "label": "modulation_control",
                    "description": "Controls the modulation amount of the signal",
                    "type": "object",
                    "keys_type": "string",
                    "properties": {
                        "x_offset": {
                            "label": "X offset percentage",
                            "type": "percentage",
                            "default": 99,
                            "min": 1,
                            "max": 99
                        },
                        "y_offset": {
                            "label": "Y offset percentage",
                            "type": "percentage",
                            "default": 99,
                            "min": 1,
                            "max": 99
                        },
                        "require": false
                    }
                },
                {
                    "label": "threshold_default",
                    "description": "A value that will pick the pixel up otherwise ignore it",
                    "type": "integer",
                    "default": 50,
                    "min": 1,
                    "max": 255
                },
                {
                    "label": "feagi_index",
                    "description": "An index to place",
                    "type": "integer",
                    "default": 0
                }
            ]
