                    "type": "string"
                },
                {
                    "label": "disabled",
                    "description": "Disables the device when true.",
                    "type": "boolean",
                    "default": false
                },
                {
                    "label": "min_value",
                    "description": "Minimum range detectable by the sensor.",
                    "type": "list",
                    "element_type": "float",
                    "default": [
                        null,
                        null,
                        null
                    ],
                    "min_length": 3,
                    "max_length": 3
                },
                {
                    "label": "max_value",
                    "description": "Maximum range detectable by the sensor",
                    "type": "list",
                    "element_type": "float",
                    "default": [
                        null,
                        null,
                        null
                    ],
                    "min_length": 3,
                    "max_length": 3
                },
                {
                    "label": "feagi_index",
