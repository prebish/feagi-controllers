                    "type": "integer",
                    "default": 0
                }
            ]
        },
        "gpio_input": {
            "label": "GPIO Input",
            "description": "",
            "parameters": [
                {
                    "label": "custom_name",
                    "description": "User defined name for the device.",
                    "type": "string"
                },
                {
                    "label": "disabled",
                    "description": "Disables the device when true.",
                    "type": "boolean",
                    "default": false
                },
                {
                    "label": "digital",
                    "description": "If true designates the GPIO input as a digital type",
                    "type": "boolean",
                    "default": false
                },
                {
                    "label": "digital_pin_configuration",
                    "description": "Captures Digital input GPIO port configuration settings",
                    "type": "object",
                    "keys_type": "integer",
                    "depends_on": "digital",
                    "properties": {
                        "custom_name": {
                            "label": "GPIO Pin number",
                            "type": "string",
                            "default": ""
                        },
                        "disable": {
                            "type": "boolean",
                            "default": false
                        }
                    }
                },
                {
                    "label": "Analog pin Configuration",
                    "description": "Captures analog input GPIO port configuration settings",
                    "type": "object",
                    "keys_type": "integer",
                    "depends_on": "!digital",
                    "properties": {
                        "max_value": {
                            "type": "float",
                            "default": 1,
                            "min": 0,
                            "max": 1
                        },
                        "min_value": {
                            "type": "float",
                            "default": 0,
                            "min": 0,
                            "max": 1
                        },
                        "disable": {
                            "type": "boolean",
                            "default": false
                        },
                        "custom_name": {
                            "type": "string",
                            "default": ""
                        }
                    }
                },
                {
                    "label": "feagi_index",
                    "description": "An index to place",
                    "type": "integer",
                    "default": 0
                }
            ]
        },
